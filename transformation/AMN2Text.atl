query 
	AMN2Text = 
		AMN!AbstractMachine.allInstances()->
			collect(c| c.toString().writeTo('C:/source/ATL_workspace/RDF2AMN/output/'+ c.name + '.mch'))->
			union(AMN!Refinement.allInstances()->
				collect(c| c.toString().writeTo('C:/source/ATL_workspace/RDF2AMN/output/'+ c.name + '.ref')))
	;
uses AMN2String;